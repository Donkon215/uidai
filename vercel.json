{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "DEBUG": "False",
    "LOG_LEVEL": "INFO",
    "ML_ENABLED": "False",
    "RATE_LIMIT_ENABLED": "True",
    "API_KEY_ENABLED": "True"
  },
  "functions": {
    "api/*.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}
