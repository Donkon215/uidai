{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "ENVIRONMENT": "production",
    "DEBUG": "False",
    "LOG_LEVEL": "INFO",
    "ML_ENABLED": "False",
    "RATE_LIMIT_ENABLED": "True",
    "API_KEY_ENABLED": "True"
  }
}
